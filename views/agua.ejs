<%- include("template/header", {WebTitle: "Inicio"}) %>



<div class="container">
    <h1 class="title">Agua</h1>



    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Mes</th>
                <th scope="col">Valor</th>
                <th scope="col">Acciones</th>
            </tr>
        </thead>
        <tbody>
            <% if (arrayAgua.length > 0) { %>
            <% arrayAgua.forEach(agua => { %>

            <tr>


                <th scope="row"><%= agua.id %> </th>
                <td><%= agua.mes %></td>
                <td><%= agua.valor %></td>
                <td>@mdo</td>
            </tr>
            <% }) %>
            <% } %>

        </tbody>
    </table>

</div>

<%- include("template/footer") %>