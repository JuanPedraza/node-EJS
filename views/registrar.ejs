<%- include("template/header", {WebTitle: "Registrar datos - Agua"}) %>

<div class="container">

    <h1 class="title">Registro</h1>


    <form action="/agua" method="POST">
        <div class="mb-3">
            <label class="form-label">Fecha Inicio</label>
            <input type="date" class="form-control" name="dateStart" value="2018-01-01" min="2018-01-01" max="2030-12-31">

        </div>
        <div class="mb-3">
            <label class="form-label">Fecha Fin</label>
            <input type="date" class="form-control" name="dateEnd" value="2018-01-01" min="2018-01-01" max="2030-12-31">

        </div>
        <div class="mb-3">
            <label class="form-label">Valor</label>
            <input type="text" class="form-control" name="value">
        </div>
        

        <button type="submit" class="btn btn-block btn-success">Registrar</button>
    </form>



</div>


<%- include("template/footer") %>